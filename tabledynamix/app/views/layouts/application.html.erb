<!DOCTYPE html>
<html>
<head>
  <title>Tabledynamix</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<p>
  <% if current_user %>
    Currently signed in as <%= current_user.email %><%= link_to "Log Out", logout_path, method: :delete %>
  <% else %>
    <%= link_to "Log in", new_sessions_path %>
  <% end %>
</p>
<%= yield %>

</body>
</html>
